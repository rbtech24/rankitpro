    "bcrypt": "^6.0.0",
    "connect-pg-simple": "^10.0.0",
    "drizzle-orm": "^0.39.3",
    "drizzle-zod": "^0.8.2",
    "esbuild": "^0.25.5",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.1",
    "helmet": "^8.1.0",
    "jszip": "^3.10.1",
    "memorystore": "^1.6.7",
    "multer": "^2.0.1",
    "pg": "^8.16.3",
    "resend": "^4.6.0",
    "stripe": "^18.3.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "uuid": "^11.1.0",
    "vite": "^6.3.5",
    "ws": "^8.18.3",
    "zod": "^3.25.67",
    "zod-validation-error": "^3.5.2"
